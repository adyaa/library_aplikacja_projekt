@model DatabaseModel.UserTypeTable

@{
    ViewBag.Title = "Typ użytkownika > Usuń";
}


<h3>Jesteś pewny, że chcesz usunąć wybrany typ użytkownika?</h3>
<div>

    <dl class="dl-horizontal">
        <dt>
            Typ użytkownika
        </dt>

        <dd>
            @Html.DisplayFor(model => model.UserType)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Usuń" class="btn btn-default" /> |
            @Html.ActionLink("Powrót", "Index")
        </div>
    }
</div>
